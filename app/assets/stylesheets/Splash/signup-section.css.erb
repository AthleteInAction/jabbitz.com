div.signup-header {
	margin-bottom: 20px;
}
div.signup-section {
	display: table;
	border-collapse: separate;
	border-spacing: 0;
	width: 100%;
}
div.signup-section div.signup-left,
div.signup-section div.signup-right {
	display: table-cell;
}
div.signup-section div.signup-left {
	width: 45%;
	padding-right: 50px;
}
div.signup-section div.signup-right {
	width: 55%;
	padding-left: 50px;
}

div.signup-section div.signup-left div.form-group {
	margin-bottom: 20px;
}


div.signup-section div.signup-left div.signup-bdayinfo {
	display: table;
	border-spacing: 0;
	border-collapse: separate;
}
div.signup-section div.signup-left div.signup-bdayinfo div.signup-bdayinfo-item {
	display: table-cell;
	width: 70px;
	padding: 0 1px;
}
div.signup-section div.signup-left div.signup-bdayinfo div.signup-bdayinfo-item:nth-child(1) {
	width: 100px;
	padding-left: 0;
}
div.signup-section div.signup-left div.signup-bdayinfo div.signup-bdayinfo-item:nth-child(2) {
	width: 60px;
}
div.signup-section div.signup-left div.signup-bdayinfo div.signup-bdayinfo-item:nth-child(3) {
	width: 65px;
}
div.signup-section div.signup-left div.signup-bdayinfo div.signup-bdayinfo-item:last-child {
	width: inherit;
	padding-left: 18px;
	padding-right: 0;
}


div.signup-section div.signup-right p {
	font-size: 14px;
	color: #6d6e70;
	margin-top: 20px;
}
div.signup-section div.signup-right p img {
	vertical-align: middle;
	margin-right: 10px;
}