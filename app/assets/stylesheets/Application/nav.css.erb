div.dashboard-nav {
	display: table;
	border-collapse: separate;
	border-spacing: 0;
	width: 100%;
	height: 100px;
}
div.dashboard-nav div.dashboard-nav-item {
	display: table-cell;
	vertical-align: middle;
	text-align: left;
}
div.dashboard-nav div.dashboard-nav-item:last-child {
	text-align: right;
}

div.dashboard-nav div.dashboard-nav-item a.logo {
	display: block;
	height: 80px;
	width: 195px;
	background-image: url(<%= asset_path 'logo-v2.png' %>);
	background-position: center center;
	background-repeat: no-repeat;
	background-size: 195px 80px;
}

div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar {
	list-style: none;
	margin: 0;
	padding: 0;
}
div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar:after {
	clear: both;
}
div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar li.nav-bar-item {
	display: inline-block;
	padding-left: 15px;
	padding-right: 15px;
	vertical-align: middle;
}
div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar li.nav-bar-item:last-child {
	padding-right: 0;
}
div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar li.nav-bar-item a.nav-bar-link {
	display: block;
	color: #FFF;
	font-size: 18px;
	text-decoration: none;
}
div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar li.nav-bar-item a.nav-bar-link:hover {
	color: red;
}
div.dashboard-nav div.dashboard-nav-item ul.dashboard-nav-bar li.nav-bar-item span.dropdown a.dropdown-toggle {
	font-size: 20px;
}
.dropdown {

}
.dropdown-menu {
	min-width: inherit;
}